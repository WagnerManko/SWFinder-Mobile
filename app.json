{
  "expo": {
    "name": "SWFinder",
    "slug": "SWFinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c1e22"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mko.swfinder",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.mko.swfinder",
        "versionCode": 1,
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1c1e22"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidNavigationBar": {
      "backgroundColor": "#1c1e22"
   }
  }
}
